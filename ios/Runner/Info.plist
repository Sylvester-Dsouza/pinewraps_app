<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Pinewraps App</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>pinewraps_app</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>pinewraps</string>
				<string>com.googleusercontent.apps.1234567890-abcdefghijklmnopqrstuvwxyz</string>
			</array>
			<key>CFBundleURLName</key>
			<string>com.pinewraps.app</string>
		</dict>
	</array>
	<key>com.apple.developer.applesignin</key>
	<array>
		<string>Default</string>
	</array>
	<key>GIDClientID</key>
	<string>1234567890-abcdefghijklmnopqrstuvwxyz.apps.googleusercontent.com</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>remote-notification</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	
	<!-- Privacy descriptions -->
	<!-- For webview_flutter_wkwebview -->
	<key>NSCameraUsageDescription</key>
	<string>This app needs camera access to scan QR codes and take photos for uploads</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>This app needs microphone access for voice input in web content</string>
	
	<!-- For url_launcher -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>https</string>
		<string>http</string>
		<string>tel</string>
		<string>mailto</string>
		<string>whatsapp</string>
	</array>
	
	<!-- For path_provider and shared_preferences -->
	<key>NSDocumentsDirectoryUsageDescription</key>
	<string>This app needs access to documents to save your preferences and cached data</string>
	<key>NSDownloadsDirectoryUsageDescription</key>
	<string>This app needs access to downloads to save files you choose to download</string>
	
	<!-- For network requests -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsArbitraryLoadsInWebContent</key>
		<true/>
	</dict>
	
	<!-- For photo library access -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs access to photo library for uploading images</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>This app needs access to photo library to save images</string>
	
	<!-- For Firebase Messaging -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app needs access to location when open to provide location-based notifications</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>This app needs access to location when in the background for location-based notifications</string>
</dict>
</plist>
